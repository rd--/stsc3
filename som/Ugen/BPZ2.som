BPZ2 = ScUgen (
----------------------------

in: in = (
    ^UgenBuilder name: 'BPZ2' numChan: 1 rateSpec: #(1) specialIndex: 0 inputArray: {in}
)

in: in mul: mul = (
    ^(BPZ2 in: in) * mul
)

in: in mul: mul add: add = (
    ^(BPZ2 in: in) * mul + add
)

primaryFactoryMethod = (
    ^#in:
)


comment = (^'
Two zero fixed midpass.
')


)

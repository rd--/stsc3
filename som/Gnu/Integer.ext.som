Integer = Integer (

<< shiftAmount = (
	shiftAmount < 0 ifTrue: [ self error: 'negative arg' ].
	^self bitShift: shiftAmount
)

>> shiftAmount = (
	shiftAmount < 0 ifTrue: [ self error: 'negative arg' ].
	^ self bitShift: 0 - shiftAmount
)

atRandom = (
	^Random between: 1 and: self
)

isFraction = (
	^true
)

isLiteral = (
	^true
)

)


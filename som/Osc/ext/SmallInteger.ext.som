SmallInteger = SmallInteger (

oscAlign = (
	"Osc data is aligned to 4-byte boundaries."
	^((self + 3) bitAnd: 3 bitInvert) - self
)

oscTypeTag = (
	"The type tag for 32-bit integers."
	^$i
)

printOscOn: aStream = (
	aStream << self asString << ' '
)

putOscOn: aStream = (
	aStream int32: self
)

)

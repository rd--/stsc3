// birdies (jmcc) #6
OverlapTexture({
    arg tr;
    var p1 = { LFPulse(0.4 + TRand(0, 1, tr), 0, TRand(0, 0.8, tr) + 0.1) * (TRand(0, 3, tr) + 4) } ;
    var p2 = LFPulse(0.2 + TRand(0, 0.5, tr), 0, 0.4) * 0.02;
    var sw = LFSaw(p1.value + p1.value + 2, 0) * (1000 + TRand(0, 800, tr)).negated + 4000 + TRand(-1200, 1200, tr);
    var freq = Lag(sw, 0.05);
    var amp = Lag(p2, 0.3);
    Pan2(SinOsc(freq, 0), TRand(-1, 1, tr), amp)
}, 7, 4, 4)

// thx simulation (geirmund simonsen)
var freqArray = [77, 74, 72, 70, 65, 62, 60, 58, 53, 50, 46, 34].midiCps;
var randomFreq = EnvSpec([1, 1, 0.007], [8, 6], [0, -4], 0, 0, 0).asEnvGen;
var ampEnv = EnvSpec([0.07, 0.07, 0.21], [8, 6], [0, 1], 0, 0, 0).asEnvGen;
freqArray.collect({
	arg item;
	var freq = LFNoise2(1.3) * 100 + 230 * randomFreq + EnvSpec([0, 0, item], [8, 6], [0, -3], 0, 0, 0).asEnvGen;
	Pan2(Saw(freq), LFNoise2(1.3), 1)
}).sum * ampEnv * 0.25

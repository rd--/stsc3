// alien froggies (jmcc) #1
OverlapTexture({
    arg tr;
    var rate = 11;
    var r = Fold(rate * TRand(-0.2, 0.2, tr).exp, 1, 30); // bilinrand
    Formant(r, TExpRand(200, 3000, tr), TRand(0, 9, tr) * r + r) * 0.05;
}, 0.5, 0.25, 5)

// alien froggies (jmcc) #1 ; filtermethods
OverlapTexture({
    arg tr;
    var r = tr.tRand(-0.2, 0.2).exp.mul(11).fold(1, 30); // bilinrand
    Formant(r, tr.tExpRand(200, 3000), tr.tRand(0, 9).mulAdd(r, r)).mul(0.05);
}, 0.5, 0.25, 5)

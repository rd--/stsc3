// <https://swiki.hfbk-hamburg.de/MusicTechnology/899> (tw ; tim walters)
var nd = {
    arg k;
    var x = {
        arg f, m, j;
        SinOsc(f + (m * 4 * j), m) * LFNoise1(j + 1 / f * 4) / 2
    };
    var y = x.value(0.1, 0, 8);
    1.to(9).do({ arg i; y = x.value(i * k * [4, 8], y, i) });
    y
};
1.to(8).mixFill(nd) / 4

// slow beating sines (jmcc) #7 ; texture=xfade,4,4,inf
var n = 20;
var d = 5.0;
var p = OrderedCollection.new(3 * n);
var q = OrderedCollection.new(3 * n);
var f = {
    arg freq;
    SinOscBank(freq, [1], { 0.0.rand(2 * pi) }.dup(3 * n))
};
n.timesRepeat({
  var freq = 24.0.rand(84.0).midiCps;
  p.add(freq);
  p.add(freq + d.rand2);
  p.add(freq + d.rand2);
  q.add(freq + d.rand2);
  q.add(freq + d.rand2);
  q.add(freq + d.rand2)
});
[p, q].collect(f) * 0.1 / n
